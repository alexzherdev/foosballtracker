{
  "extends": "eslint:recommended",

  "rules": {
    "no-unused-vars": 0,
    "no-console": 0
  },

  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    }
  }
}